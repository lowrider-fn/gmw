{
  "name": "gmw",
  "version": "1.0.0",
  "description": "GMW",
  "author": "lowrider-fn",
  "private": true,
  "scripts": {
    "eslint": "eslint --fix --ext .js,.ts,.tsx,.vue .",
    "stylelint": "stylelint **/*.{scss,vue} --fix",
    "cont": "docker container ls -aq",
    "cont:stop": "docker container stop $(docker container ls -aq)",
    "cont:rm": "docker container rm $(docker container ls -aq)",
    "img": "docker image ls -aq",
    "img:rm": "docker rmi $(docker images -q)",
    "img:rm-a": "docker image prune -a",
    "vol:rm-a": "docker volume prune",
    "rm-a": "docker system prune",
    "net": "docker network ls",
    "net:rm-a": "docker network prune",
    "up": "docker-compose up",
    "up:build": "docker-compose up --build",
    "db:dump": "mongodump --forceTableScan -d gmw -o ./back/db",
    "db:dump-up": "mongorestore --drop --verbose --db gmw ./back/db/gmw/",
    "heroku-postbuild": "docker-compose up --build"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "stylelint": "^13.5.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.17.2",
    "vue-clickaway": "^2.2.2"
  }
}
